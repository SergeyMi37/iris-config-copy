              "parameters": [],
              "produces": [
                "application/json"
              ],
              "responses": {
                  "200": {
                      "description": "OK"
                  }
              }
          }
      },
      "/export/tasks/": {
          "get": {
              "description": "Export all users defined tasks in XML format.",
              "operationId":"ExportTasks",
              "parameters": [],
